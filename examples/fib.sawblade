block "fib" :: (%n) {
    %cmp = sub %n 0;
    %f = flags %cmp z;
    br %f @fib_early_ret(%n) @check_1(%n)
}

block %check_1 :: (%n) {
    %c = sub %n 1;
    %f = flags %cmp z;
    br %f @fib_early_ret(%n) @run_fib_recursive(%n)
}

block %fib_early_ret :: (%n) {
    %n
}

block %run_fib_recursive :: (%n) {
    %a = sub %n 1;
    %b = sub %n 2;
    %c = call @fib %a;
    %d = call @fib %b;
    add %c %d
}

block "main" :: { return [rax] } {
    %n = 5
    call @fib 5
}
